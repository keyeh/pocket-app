{
    "extends": "eslint:recommended",
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 8,
        "sourceType": "module",
        "ecmaFeatures": {
            "experimentalObjectRestSpread": true
        }
    },
     "env": {
            "es6": true,
            "node": true,
            "browser": true
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "no-console": 0
    }
}
